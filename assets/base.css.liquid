}
{% if settings.custom_background_image %}
html, body {
 {% if settings.background_image_style == "repeat" %}
 background: {{ settings.main_background_color }} url('{{ 'background_image.png' | asset_url }}') repeat scroll; 
{% endif %}
{% if settings.background_image_style == "no-repeat" %}
 background: {{ settings.main_background_color }} url('{{ 'background_image.png' | asset_url }}') no-repeat scroll; 
{% endif %}
}
{% endif %}
body {
  padding-top: 60px; 
  padding-bottom: 60px;
}
